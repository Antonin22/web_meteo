{
  "name": "myapp",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "watch": "nodemon",
    "scriptjs": "node ./script.js",
    "dev": "vite",
    "build": "vite build",
    "dev:all": "concurrently \"npm run start\" \"npm run dev\"",
    "setup": "npm install && npm run build",
    "production": "concurrently \"npm run scriptjs\" \"npm run start\""
  },
  "dependencies": {
    "@influxdata/influxdb-client": "^1.35.0",
    "chart.js": "^4.4.8",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "express": "^4.21.2",
    "http-errors": "~1.6.3",
    "leaflet": "^1.9.4",
    "morgan": "~1.9.1",
    "nodemon": "^3.1.9",
    "pug": "^3.0.3",
    "vue-chartjs": "^5.3.2"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/cli-service": "^5.0.8",
    "concurrently": "^9.1.2",
    "vite": "^6.2.0",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0"
  }
}